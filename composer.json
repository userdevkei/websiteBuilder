{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
  "require": {
    "php": "^8.0.2",
    "laravel/framework": "^9.52.20",
    "laravel/sanctum": "^3.0",
    "laravel/tinker": "^2.7",
    "laravel/socialite": "^5.5",
    "laravel/ui": "^4.0",
    "maatwebsite/excel": "^3.1",
    "mews/purifier": "^3.3",
    "monolog/monolog": "^2.3|^3.0",
    "pusher/pusher-php-server": "^5.0|^6.0|^7.0",
    "razorpay/razorpay": "2.*",
    "stripe/stripe-php": "^7.0|^8.0|^9.0|^10.0",
    "yajra/laravel-datatables": "^9.0|^10.0",
    "ext-curl": "*",
    "ext-ftp": "*",
    "anhskohbo/no-captcha": "^3.4",
    "barryvdh/laravel-dompdf": "^2.0",
    "breadhead/paddle": "^1.2",
    "doctrine/dbal": "^3.0",
    "fideloper/proxy": "^4.4",
    "flutterwavedev/flutterwave-v3": "^1.1",
    "guzzlehttp/guzzle": "^7.0",
    "intervention/image": "^2.7",
    "laravel/cashier-paddle": "^1.8"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.5.10",
    "spatie/laravel-ignition": "^1.0"
  },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
		"files": [
            "app/Helpers/general.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
